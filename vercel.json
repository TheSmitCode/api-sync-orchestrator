{
  "version": 2,
  "functions": {
    "api/**/*.py": {
      "runtime": "python3.12",
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/main.py"
    }
  ],
  "env": {
    "STRIPE_SECRET_KEY": "@STRIPE_SECRET_KEY",
    "PORT": "8000"
  }
}
